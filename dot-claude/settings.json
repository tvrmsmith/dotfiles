{
  "env": {
    "CLAUDE_CODE_USE_VERTEX": "1",
    "CLOUD_ML_REGION": "us-east5",
    "ANTHROPIC_VERTEX_PROJECT_ID": "wsh-dev-vertex-wsky"
  },
  "permissions": {
    "allow": [
      "Bash(ls:*)",
      "Bash(docker build:*)",
      "Bash(git add:*)",
      "Bash(grep:*)",
      "Bash(docker run:*)",
      "Bash(docker logs:*)",
      "Bash(curl:*)",
      "Bash(docker stop:*)"
    ],
    "defaultMode": "default"
  },
  "hooks": {
    "Notification": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/hooks/send-teams-notification.sh notification"
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/hooks/send-teams-notification.sh stop"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "claude-powerline --theme=tokyo-night --style=powerline"
  },
  "enabledPlugins": {
    "shared@wellsky-plugins": true,
    "bug-fix@wellsky-plugins": true,
    "unit-gen@wellsky-plugins": true,
    "atlassian@claude-plugins-official": true,
    "commit-commands@claude-plugins-official": true,
    "humanlayer@wellsky-plugins": true,
    "warp@claude-code-warp": false,
    "claude-notifications-go@claude-notifications-go": true
  },
  "syntaxHighlightingDisabled": false,
  "alwaysThinkingEnabled": true,
  "extraKnownMarketplaces": {
    "wellsky-plugins": {
      "source": {
        "source": "git",
        "url": "git@github.com:mediwareinc/wellsky-claude-code-plugins.git"
      },
      "autoUpdate": true
    },
    "claude-code-warp": {
      "source": {
        "source": "github",
        "repo": "warpdotdev/claude-code-warp"
      }
    },
    "claude-notifications-go": {
      "source": {
        "source": "github",
        "repo": "777genius/claude-notifications-go"
      }
    }
  }
}
